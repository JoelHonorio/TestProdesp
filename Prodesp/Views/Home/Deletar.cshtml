@model Prodesp.ViewModels.ImunobiologicoViewModel

@{
    ViewData["Title"] = "Delete";
}

<h3>Tem certeza que quer deletar este registro?</h3>

<div>
    <hr />

    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.AnoLote)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.AnoLote)
        </dd>
    </dl>

    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Fabricante)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.FabricanteString)
        </dd>
    </dl>

    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Descricao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
    </dl>

    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataCriacao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataCriacaoString)
        </dd>
    </dl>
    
    <form asp-action="Deletar">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Deletar" class="btn btn-danger" />
        <a class="btn btn-warning" asp-action="Index"><spam class="fa fa-long-arrow-alt-left"></spam></a>
    </form>
</div>